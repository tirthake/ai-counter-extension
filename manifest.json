{
  "manifest_version": 3,
  "name": "AI visit counter",
  "version": "2.1",
  "description": "Counts every visit to a specific webpage.",
  "permissions": ["webNavigation", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Visit Counter",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/logo.png",
    "32": "icons/logo.png",
    "48": "icons/logo.png",
    "128": "icons/logo.png"
  }

}